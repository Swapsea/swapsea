<!DOCTYPE html>
<html>
	<head>
		<title><%= content_for?(:title) ? yield(:title) : "Swapsea" %></title>
		<meta charset="utf-8">
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
		<meta name="apple-mobile-web-app-capable" content="yes" />
		<meta name="description" content="<%= content_for?(:description) ? yield(:description) : "Swapsea" %>">
		<meta name="keywords" content="Swapsea">
		<%= stylesheet_link_tag "basic-layout", :media => "all" %>
		<%= render 'application/favicon' %>
		<%= csrf_meta_tags %>
		<%= yield(:head) %>
	</head>
	<body>
		<div class="navbar navbar-default navbar-fixed-top container-fluid" role="navigation">
			<ul class="nav navbar-nav hidden-xs">
				<li>
			         <span>
			         	<button type="reset" class="btn btn-default" onclick="window.history.back();">Back</button>
			         </span>
			    </li>
			</ul>
			<ul class="navbar-logo-container">
				<a class="navbar-logo" href="/"><h1>Swapsea</h1></a>
			</ul>
		</div>
		<div class="container">

			<%= render 'layouts/messages' %>

			<%= yield %>

			<%= render 'home/footer_large' %>
			<%= javascript_include_tag "basic-layout" %>
			<%= yield :scripts %>
			<script type="text/javascript">
			  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
			  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
			  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
			  })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

			  ga('create', 'UA-55037512-1', 'auto');
			  ga('send', 'pageview');
		</script>
		</div>
	</body>
</html>
