<!-- Google tag (gtag.js) -->
<% if ENV['GA_4_CODE'] %>
<script async src="https://www.googletagmanager.com/gtag/js?id=<%= ENV['GA_4_CODE'] %>"></script>
<script>
  window.dataLayer = window.dataLayer || [];
  function gtag(){dataLayer.push(arguments);}
  gtag('js', new Date());

  gtag('config', '<%= ENV['GA_4_CODE'] %>');
</script>
<% end %>
<!-- End Google Analytics -->
