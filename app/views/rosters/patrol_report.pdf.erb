<div id="report">
  <%= image_tag wicked_pdf_asset_base64('alternate-logo.png'), height: "75" , alt: "Swapsea" , class: "logo" %>
    <div class="patrol">
      <h1>
        <%= @roster.patrol.club.name %>
      </h1>
      <h2>
        <%= @roster.patrol.name %>
      </h2>
      <div class="details">
        <div class="date">
          <%= @roster.start.strftime("%A, %-d %B %Y") %>
        </div>
        <div class="time">
          <%= @roster.start.strftime("%H:%M") %> - <%= @roster.finish.strftime("%H:%M") %>
        </div>
        <div class="created-at">Correct at: <%= Time.zone.now %>
        </div>
      </div>
    </div>
    <!--/.patrol -->
    <div class="members">
      <div class="patrol-report">
        <table class="table">
          <tr>
            <th class="position">Position</th>
            <th class="patroller">Patroller</th>
            <th class="sub">Substitute</th>
            <th class="start center">Start Time</th>
            <th class="finish center">Finish Time</th>
            <th class="total center">Total Time</th>
            <th class="sign center">Signature</th>
          </tr>
          <%= render partial: 'report_membership' , locals: {r: @roster, ru: @roster.members } %>
            <% 4.times do %>
              <tr>
                <td>Walk-in</td>
                <td></td>
                <td></td>
                <td></td>
                <td></td>
                <td></td>
                <td></td>
              </tr
              <% end %>
        </table>
      </div>
    </div>
