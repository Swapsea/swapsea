<%= simple_form_for(@outreach_patrol) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <% if selected_user.has_role?(:admin)  %>
      <%= f.association :club, label_method: :name, value_method: :name, include_blank: false %>
    <% end %>
    <%= f.input :location %><br/>
    <%= f.input :start %><br/>
    <%= f.input :finish %><br/>
  </div>

  <div class="form-actions">
    <%= f.button :submit, :class => 'btn btn-default' %>
  </div>
<% end %>
